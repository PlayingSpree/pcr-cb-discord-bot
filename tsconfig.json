{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "lib": [
      "es2021"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "out"
  },
  "exclude": [
    "node_modules"
  ]
}